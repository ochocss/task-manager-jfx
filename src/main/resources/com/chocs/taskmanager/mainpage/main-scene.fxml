<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" maxHeight="700.0" maxWidth="1600.0" minHeight="400.0" minWidth="400.0" 
      spacing="15.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" 
      fx:controller="com.chocs.taskmanager.mainpage.MainController">
    <padding>
        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
    </padding>
   <ScrollPane fx:id="scrollPane" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
    <Button onAction="#onCreateButtonClick" prefHeight="35.0" prefWidth="222.0" text="Create new task" />
</VBox>
